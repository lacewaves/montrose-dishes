<%= simple_form_for @restaurant do |f| %>

  <%= f.input :name, label: "Restaurant Name" %>
  <%= f.input :cuisine, label: "Cuisine?", as: :select, collection: ["Greek", "Italian", "American", "Appalachian", "Cajun", "Canadian", "Circus Clown", "Postal Service"] %>
  <%= f.input :placeId, label: "Google Maps PlaceId?" %>
  
  <%= f.submit "Create Restaurant" %>

<% end %>
